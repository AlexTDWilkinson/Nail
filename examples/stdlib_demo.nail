// Stdlib demo - shows math, string, and array functions

// Math functions
sqrt_result:f = math_sqrt(16.0);
print(string_from(sqrt_result));

// String functions
upper_result:s = string_to_uppercase(`hello`);
print(upper_result);

// Array functions
test_array:a:i = [1, 2, 3];
array_length:i = array_len(test_array);
print(string_from(array_length));

// Random number
random_num:f = math_random();
print(string_from(random_num));