// Test nested loops with break
// Since Nail has immutable variables, we can't count iterations
// We just test that nested loops work

print(`Testing nested loops`);

loop {
    print(`Outer loop`);
    
    loop {
        print(`  Inner loop iteration`);
        break; // Exit inner loop
    }
    
    print(`Back to outer loop`);
    break; // Exit outer loop
}

print(`Nested loops completed`);