// Fibonacci example from the website

// Calculate fibonacci sequence
f fib(num:i):i {
    result:i = if {
        num <= 1 => { r num; },
        else => { r fib(num - 1) + fib(num - 2); }
    };
    r result;
}

// Generate first 10 fibonacci numbers  
nums:a:i = danger(range(0, 10));
sequence:a:i = map idx in nums {
    y fib(idx);
};

print(array_join([`Fibonacci sequence: `, string_from_array_i64(sequence)], ``));