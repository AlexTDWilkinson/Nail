// Test simple reduce operation
numbers:a:i = [1, 2, 3, 4, 5];

// Sum all numbers using reduce
sum:i = reduce acc num in numbers from 0 {
    y acc + num;
};

print(array_join([`Sum: `, string_from(sum)], ``));

// Product of all numbers
product:i = reduce acc num in numbers from 1 {
    y acc * num;
};

print(array_join([`Product: `, string_from(product)], ``));