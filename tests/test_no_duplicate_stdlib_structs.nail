// Test that stdlib structs are not redefined in generated code
// This test should transpile without creating a duplicate HTTP_Route struct

// Use HTTP_Route from stdlib
test_route:HTTP_Route = HTTP_Route {
    path = `/test`,
    content = `Test content`,
    content_type = `text/plain`,
    status_code = 200
};

// Create a local struct with a different name
struct MyCustomStruct {
    name:s,
    value:i
}

// Use the local struct
my_struct:MyCustomStruct = MyCustomStruct {
    name = `example`,
    value = 123
};

// Both should work without conflicts
print(`Stdlib struct path: `, test_route.path);
print(`Local struct name: `, my_struct.name);

print(`No duplicate struct definitions!`);